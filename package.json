    "test": "NODE_ENV=test substrate-exec-jest --detectOpenHandles"
  },
  "keywords": [],
  "author": "Parity Technologies <admin@parity.io>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@polkadot/api": "10.11.2",
    "@polkadot/apps-config": "0.132.1",
    "@substrate/dev": "0.7.1",
    "@types/node-fetch": "^2.6.9",
    "node-fetch": "2.6.7"
  },
  "packageManager": "yarn@4.1.0"
}
